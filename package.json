    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "coffee-script": "1.10.0",
    "istanbul": "^0.4.4",
    "mimus": "^0.2.2",
    "mocha": "^2.5.3",
    "npm-check-updates": ">=2.6.8",
    "pryjs": "^1.0.2",
    "release-it": "^2.4.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
