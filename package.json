    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coffee-script": "1.11.0",
    "istanbul": "^0.4.5",
    "mimus": "^0.2.3",
    "mocha": "^3.1.0",
    "npm-check-updates": ">=2.8.0",
    "pryjs": "^1.0.2",
    "release-it": "^2.4.3",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
