{
  "name": "@forthright/vile-eclint",
  "version": "0.1.8",
  "description": "A vile plugin for eclint",
  "license": "MPL-2.0",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib",
    "src"
  ],
  "scripts": {
    "compile": "babel -d lib -m common src",
